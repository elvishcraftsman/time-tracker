<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="time-tracker">
	<schema id="com.lynnmichaelmartin.TimeTracker" path="/com/lynnmichaelmartin/TimeTracker/">
	  <key name="window-width" type="i">
      <default>350</default>
    </key>
    <key name="window-height" type="i">
      <default>500</default>
    </key>
    <key name="window-maximized" type="b">
      <default>false</default>
    </key>
    <key name="firstdayofweek" type="i">
      <default>0</default>
    </key>
    <key name="numberofbackups" type="i">
      <default>7</default>
    </key>
    <key name="syncinterval" type="i">
      <default>5</default>
    </key>
    <key name="addprojectsfromlog" type="b">
      <default>true</default>
    </key>
    <key name="projects" type="s">
      <default>''</default>
    </key>
    <key name="log" type="s">
      <default>''</default>
    </key>
    <key name="ampmformat" type="b">
      <default>false</default>
    </key>
    <key name="autotemplog" type="b">
      <default>false</default>
    </key>
    <key name="version" type="s">
      <default>'2.1.2'</default>
    </key>
    <key name="reports" type="s">
      <default>'Today`day+0`day+0`````project`/~/`This Week`week+0`week+0`````client`project`/~/`Last Week`week-1`week-1`````client`project'</default>
    </key>
    <key name="resetproject" type="b">
      <default>true</default>
    </key>
    <key name="resetdescription" type="b">
      <default>true</default>
    </key>
	</schema>
</schemalist>
